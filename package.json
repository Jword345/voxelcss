{
    "name": "voxelcss",
    "version": "0.1.9",
    "homepage": "https://voxelcss.js.org",
    "author": "Jason Wohlgemuth",
    "license": "MIT",
    "description": "Voxel.css with updated project architecture, easier contributing, and npm availability",
    "main": "./dist/voxelcss.js",
    "keywords": [
        "voxel",
        "voxels",
        "css",
        "fun",
        "browser"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jhwohlgemuth/voxelcss.git"
    },
    "engines": {
        "node": "*"
    },
    "babel": {
        "presets": [
            "env",
            "babili"
        ]
    },
    "jest": {
        "setupFiles": [
            "jest-localstorage-mock"
        ]
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-preset-babili": "^0.1.4",
        "babel-preset-env": "^1.6.0",
        "browserify": "^14.4.0",
        "browserify-css": "^0.11.0",
        "coveralls": "^2.13.1",
        "cssnano": "^3.10.0",
        "eslint": "^4.2.0",
        "eslint-config-omaha-prime-grade": "^4.0.0",
        "eslint-plugin-compat": "^1.0.4",
        "jest": "^20.0.4",
        "jest-localstorage-mock": "^1.2.0",
        "jsdoc": "^3.5.3",
        "minami": "^1.2.3",
        "postcss": "^6.0.8",
        "postcss-cli": "^4.1.0",
        "postcss-cssnext": "^3.0.2",
        "postcss-import": "^10.0.0",
        "postcss-reporter": "^4.0.0",
        "postcss-safe-parser": "^3.0.1",
        "watch": "^1.0.2"
    },
    "scripts": {
        "dev": "stmux --mouse --title 'Voxelcss dev task' [ [ \"npm run lint:watch\" : [ \"npm run build:watch\" .. \"npm run demo\" ] ] .. \"npm run test:watch -- --coverage\" ]",
        "demo": "npm run build && npm run browsersync",
        "docs": "jsdoc ./lib/*.js --destination ./docs --readme ./README.md --template ./node_modules/minami",
        "lint": "eslint -c ./.eslintrc.js ./lib/*.js --fix",
        "lint:watch": "watch 'npm run lint' ./lib",
        "lint:tests": "eslint -c ./.eslintrc.js ./test/*.js",
        "test": "jest test/.*.test.js",
        "test:watch": "npm test -- --watch",
        "test:ci": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "prebuild": "postcss ./assets/voxelcss.css -d ./dist",
        "build": "browserify -g browserify-css -e ./lib/index.js ./lib/index.js --standalone voxelcss -o ./dist/voxelcss.js",
        "build:watch": "watch 'npm run build && browser-sync reload --port 1337' ./lib ./assets",
        "postbuild": "babel ./dist/voxelcss.js --out-file ./dist/voxelcss.js",
        "preversion": "npm test",
        "postversion": "git push && git push --tags",
        "delete:remote": "git push origin --delete gh-pages && git fetch --all --prune",
        "delete:docs": "git rm -r --cached docs",
        "predelete": "npm run delete:remote",
        "delete": "npm run delete:docs",
        "predeploy": "npm run docs && cp CNAME docs && git add docs -f && git commit -m Deployment",
        "deploy": "git subtree push --prefix docs origin gh-pages",
        "coverage": "npm test -- --coverage",
        "test:travis": "npm run test:ci",
        "browsersync": "browser-sync start --server ./test/demo --serveStatic . --files test/demo --open ./test/demo/index.html --port 1337"
    },
    "dependencies": {}
}
